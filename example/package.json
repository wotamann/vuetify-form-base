{
  "name": "vuetify-form-base-example",
  "version": "0.2.6",
  "private": true,
  "description": "Form Generator as Vue component based on Vuetify",
  "author": "wotamann@github",
  
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:vue & npm run build:cli & npm run copy",
    "build:vue": "vue-cli-service build --dest ../dist/app",
    "build:cli": "vue-cli-service build --dest ../umd --target lib --name vFormBase ./src/components/vFormBase.vue",
    "build:wc": "vue-cli-service build --dest ../dist/wc --target lib --name v-form-base ./src/components/vFormBase.vue",
    "copy": "node ./scripts/copy.js",
    "test": "echo \"No test specified\"",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.5",
    "css-loader": "^3.5.3",
    "lodash": "^4.17.19",
    "rollup": "^2.23.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass-loader": "^9.0.2",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.11",
    "vue-the-mask": "^0.11.1",
    "vuetify": "^2.3.4"
  },
  "devDependencies": {
    "@mdi/font": "^4.9.95",
    "@mdi/js": "^4.9.95",
    "@vue/cli-plugin-babel": "^4.4.4",
    "@vue/cli-plugin-eslint": "^4.4.4",
    "@vue/cli-service": "^4.4.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.11"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "homepage": "https://github.com/wotamann/vuetify-form-base",
  "keywords": [
    "Vue",
    "Vuetify",
    "Schema",
    "Formgenerator",
    "Formbuilder",
    "JS"
  ],
  "eslintConfig": {
    "root": false,
    "env": {
      "node": false,
      "browser": false
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {}
  },
  "license": "MIT"
}
